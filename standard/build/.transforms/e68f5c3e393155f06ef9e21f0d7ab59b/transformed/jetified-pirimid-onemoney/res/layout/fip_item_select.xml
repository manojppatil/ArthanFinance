<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/fip_relative">
    <LinearLayout
        android:id="@+id/fip_linear"
        android:layout_margin="@dimen/dimen_5"
        android:background="@drawable/ic_fip_border"
        android:layout_width="wrap_content"
        android:padding="@dimen/dimen_13"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <androidx.appcompat.widget.AppCompatCheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:clickable="false"
            android:enabled="false"
            android:gravity="center"
            style="@style/CheckboxStyle"
            android:id="@+id/check_fip"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/dimen_100"
            android:layout_height="@dimen/dimen_30"
            android:id="@+id/fip_logo"
            android:scaleType="fitStart"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_7"
            app:srcCompat="@drawable/ic_empty_state"
            tools:ignore="VectorDrawableCompat" />
    </LinearLayout>
</RelativeLayout>